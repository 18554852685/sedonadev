//@niagaraIcon="module://icons/x16/io.png"
class MQTTINIT
  extends Component    
{
define Log log
//This should call only one time
	virtual override void start()
	{
	log.message("MQTT Initializer app is started!\n");
	++enabled;
		if(enabled == 1)
		{
		MqttDev.doMqttInit_pub()
		MqttDev.doMqttInit_sub()
		}
	}

static int enabled = 0;
}
